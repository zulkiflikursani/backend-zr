{
  "version": 2,
  "name": "backend-node",
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(routes|contorller)/(.*)",
      "dest": "/$1/$2"
    }
  ]
}
